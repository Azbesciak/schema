{
    "version": "0.2.0",
    "configurations": [    
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: profiler for Current File",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "args": [
                "-o", "C:\\temp\\temp.prof", "${file}"
            ]
        },
        {
            "name": "-h",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["SnapTraces", "--help", ]
        },              

        {
            "name": "Match: traces manual",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "args": [
                "-o", "C:\\temp\\temp.prof", "${file}", 
                "SnapTraces",
                "--input-to-match", "gers/examples/python/data/macon-manual-traces.geojson",
                "--input-overture", "gers/examples/python/data/overture-transportation-macon.geojson",
                "--allow_loops", "True",
                "--max_point_to_road_distance", "30",
                "--output", "gers/examples/python/data/.output-macon-manual-traces-match-result.json",
            ]
        },     
        {
            "name": "Match: traces osm",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "args": [
                "-o", "C:\\temp\\temp.prof", "${file}", 
                "SnapTraces",
                "--input-to-match", "gers/examples/python/data/macon-osm-traces-combined.geojson",
                "--input-overture", "gers/examples/python/data/overture-transportation-macon.geojson",
                "--resolution", "12",
                "--allow_loops", "True",
                "--max_point_to_road_distance", "10",
                "--output", "gers/examples/python/data/.output-macon-osm-traces-combined-match-result.geojson",
            ]
        },      
        {
            "name": "Match: traces osm - no profiling",
            "type": "python",
            "request": "launch",
            "program": "${file}", 
            "console": "integratedTerminal",
            "args": [
                "SnapTraces",
                "--input-to-match", "gers/examples/python/data/macon-osm-traces-combined.geojson",
                "--input-overture", "gers/examples/python/data/overture-transportation-macon.geojson",
                "--resolution", "12",
                "--allow_loops", "True",
                "--max_point_to_road_distance", "10",
                "--output", "gers/examples/python/data/.output-macon-osm-traces-combined-match-result.geojson",
            ]
        },                   
        {
            "name": "Match: roads - no profiling",
            "type": "python",
            "request": "launch",
            "program": "${file}", 
            "console": "integratedTerminal",
            "args": [
                "MatchGeneric",
                "--input-to-match", "gers/examples/python/data/macon-line-segments.csv",
                "--input-overture", "gers/examples/python/data/overture-transportation-macon.geojson",
                "--resolution", "12",
                "--buffer", "0.0001",
                "--output", "gers/examples/python/data/.output-macon-line-segments-match-results.json",
            ]
        },
    ]
}