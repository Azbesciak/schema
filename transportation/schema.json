{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://overturemaps.org/schema-wg/transportation-schema.json",
  "title": "Overture Transportation Segment Feature",
  "description": "Prototype schema for Overture Transportation layer features as GeoJSON LineStrings",
  "type": "object",
  "allOf": [{ "$ref": "https://geojson.org/schema/Feature.json" }],
  "unevaluatedProperties": false,
  "properties": {
    "geometry": {
      "allOf": [{ "$ref": "https://geojson.org/schema/LineString.json"}]
    }
  }
}
